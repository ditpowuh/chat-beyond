{
  "name": "chat-beyond",
  "version": "0.6.0",
  "description": "A web interface to use OpenAI's API, made to be familiar and easy.",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun index.ts --tsconfig-override tsconfig.bun.json",
    "build:windows": "bun build index.ts --compile --target=bun-windows-x64 --outfile output/ChatBeyond-windows-x64",
    "build:mac": "bun build index.ts --compile --target=bun-darwin-arm64 --outfile output/ChatBeyond-macos-arm64",
    "check": "bun x tsc -p tsconfig.bun.json",
    "node:start": "node dist/index.js",
    "node:compile": "tsc -p tsconfig.node.json"
  },
  "author": "Ditpowuh",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/socket.io": "^3.0.1",
    "typescript": "^5"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "image-size": "^2.0.2",
    "mammoth": "^1.11.0",
    "marked": "^17.0.1",
    "marked-katex-extension": "^5.1.6",
    "open": "^11.0.0",
    "openai": "^6.15.0",
    "pdf-parse": "^2.4.5",
    "socket.io": "^4.8.1",
    "tiktoken": "^1.0.22",
    "uuid": "^13.0.0"
  },
  "engine": {
    "bun": ">=1.3.4"
  }
}
